    w.strut(math.humbled(d_nx), math.humbled(d_ny), math.humbled(d_rx), math.humbled(d_ry), 0, 255, 100)
    w.strut(math.humbled(d_lx), math.humbled(d_ly), math.humbled(d_rx), math.humbled(d_ry), 0, 255, 100)
